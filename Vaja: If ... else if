#include <stdio.h>

int main() {

  int x = 4;
  if (x == 1) {
    printf("ocena: negativna\n");
  } 
  
  else if(x == 2) {
    printf("ocena: zadostna\n");
  }
    else if(x == 3) {
      printf("ocena: dobra\n");
    }
      else if(x == 4) {
        printf("ocena: pravdobro\n");
      }
        else if(x == 5) {
          printf("ocena: odlicno\n");
        }


  return 0;
  }
