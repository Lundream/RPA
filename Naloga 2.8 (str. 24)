#include <stdio.h>

int main() {

  int x, y, z, x1, x2;

  printf("Enter the first number: ");
  scanf("%d", &x);
  printf("Enter the second number: ");
  scanf("%d", &y);
  printf("Enter the third number: ");
  scanf("%d", &z);

  x1 = (x && y || z);
  x2 = (x && (y || z));

  printf("Rezultat (x && y || z) je: %d\n", x1);
  printf("Rezultat (x && (y || z)) je: %d", x2);

  return 0;
}
